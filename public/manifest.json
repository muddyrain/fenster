{
  "name": "feaster",
  "description": "a simple extension to manage tabs",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "./logo.png",
    "32": "./logo.png",
    "48": "./logo.png",
    "128": "./logo.png"
  },
  "host_permissions": [
    "https://developer.chrome.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
